<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppTandT.Pages.PlanPages.AddPlanPage">
    <ContentPage.Content>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="50"/>
            </Grid.RowDefinitions>
            <Label Text="Task" TextColor="Black" Grid.Column="0" Grid.Row="0"/>
            <Label Text="DateStart" TextColor="Black" Grid.Column="0" Grid.Row="1"/>
            <Label Text="DateFinish" TextColor="Black" Grid.Column="0" Grid.Row="2"/>
            <Label Text="About" TextColor="Black" Grid.Column="0" Grid.Row="3"/>
            <ViewCell Grid.Column="0"/>
            <Button>
                
            </Button>
        </Grid>
    </ContentPage.Content>
</ContentPage>